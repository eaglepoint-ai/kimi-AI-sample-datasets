{
  "name": "task-queue-test-suite",
  "version": "1.0.0",
  "description": "Comprehensive test suite for TaskQueue with meta-testing",
  "scripts": {
    "test": "jest repository_after/tests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:meta": "jest tests/meta-test.test.ts",
    "test:all": "jest",
    "evaluation": "ts-node evaluation/evaluation.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "repository_after/**/*.ts",
      "!repository_after/**/*.test.ts"
    ],
    "testTimeout": 10000
  }
}
